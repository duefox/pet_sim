config_version=5

[application]

config/name="萌宠经营模拟器"
run/main_scene="uid://3ia6qn373kw0"
config/features=PackedStringArray("4.5", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

PetManager="*res://resource/managers/pet_manager.gd"
AudioManager="*res://resource/managers/audio/audio_manager.gd"
EventManager="*res://resource/managers/event/event_manager.gd"
ResManager="*res://resource/managers/resource/resource_manager.gd"
SceneManager="*res://resource/managers/scene/scene_manager.gd"
SaveSystem="*res://addons/save_system/save_manager.gd"
Tooltips="*res://resource/utils/tooltips_manager.gd"
MouseEvent="*res://resource/managers/event/mouse_event.gd"
GlobalData="*res://resource/managers/global/global_data.gd"
Utils="*res://resource/utils/utils.gd"
PriceManager="*res://resource/managers/price_manager.gd"
InputManager="*res://resource/managers/input_manager.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/always_on_top=true
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray("res://addons/gdscript_formatter/plugin.cfg", "res://addons/save_system/plugin.cfg", "res://addons/script-ide/plugin.cfg")

[gui]

theme/default_font_hinting=2
theme/custom="uid://cwy04bxyvv2hj"
